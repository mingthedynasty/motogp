package com.season.utils {

  import flash.events.*;
  
  /**********************
   *
   * SequenceStep
   *
   **********************/
  public class SequenceStep {
    
    /**********************
     * VARIABLES
     **********************/
    public var source:EventDispatcher;
    public var event:String;

    public var entry:Function;

    /**********************
     * CONSTRUCTOR
     **********************/
    public function SequenceStep(theSource:EventDispatcher,
                                 theEntry:Function,
                                 theEvent:String = "") {
      source = theSource;
      event = theEvent;
      entry = theEntry;
    }
  }
}
